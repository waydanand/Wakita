{
  "name": "wakita",
  "version": "1.0.0",
  "description": "Real Estate website assigned to Wakita & Associates",
  "main": "index.js",
  "scripts": {
    "install": "run-p install:server install:client",
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "start": "run-p start:server start:client",
    "start:server": "npm run start --prefix server",
    "start:client": "npm run start --prefix client",
    "build": "run-p build:server build:client",
    "build:server": "cd server && npm run build && cd ..",
    "build:client": "cd client && npm run build && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "opn-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waydanand/Wakita.git"
  },
  "author": "Daniel Ward, Wayne Song, Andre Holguin",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/waydanand/Wakita/issues"
  },
  "homepage": "https://github.com/waydanand/Wakita#readme"
}
